{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "LAB1",
            "command": "gcc",
            "args": [
                "-g",
                "${file}",
                "-lm",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-lpthread"
            ],
            "options": {
                "cwd": "/usr/bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
              }
        }
    ]
}